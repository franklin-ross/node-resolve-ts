{
  "name": "node-resolve-ts",
  "version": "1.0.2",
  "description": "NodeJS hook to resolve `.ts` files when a `.js` file is imported so that --experimental-strip-types works",
  "type": "module",
  "scripts": {
    "test": "cd tests; npm run test",
    "prepublishOnly": "npm test"
  },
  "keywords": [
    "node",
    "typescript",
    "ts",
    "experimental-strip-types",
    "resolve"
  ],
  "author": "Franklin Ross",
  "license": "ISC",
  "files": [
    "*.js"
  ],
  "exports": {
    "./register": "./register-prefer-ts.js",
    "./register-prefer-js": "./register-prefer-js.js",
    "./register-prefer-ts": "./register-prefer-ts.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/franklin-ross/node-resolve-ts.git"
  }
}
